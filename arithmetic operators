#addition
x = 5
y = 5
print(x + Y)
#subtraction
x = 10
y = 20
print(x - y)
#multiplication
x = 5
y = 5
print(x * y)
#division
x = 12
y = 3
print(x / y)
#modulus
x = 5
y = 2
print(x % y)
#exponentation
x = 2
y = 5
print(x ** y)#same as 2*2*2*2*2
#floor division
x = 15
y = 2
print(x // y)#the floor division // rounds the result down to the nearest whole number
